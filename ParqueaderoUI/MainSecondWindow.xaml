<Window
    x:Class="ParqueaderoUI.MainSecondWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="using:ParqueaderoUI.Viewss">

    <Grid>
        <StackPanel Orientation="Horizontal">
            <StackPanel Background="#2D2D30" Width="200">
                <TextBlock Text="Menú" FontSize="20" Foreground="White" Margin="10"/>
                <Button Content="⚙️ Administración" Click="Admin_Click" Margin="10"/>
                <Button Content="🚗 Nuevos Registros" Click="Ingreso_Click" Margin="10"/>
            </StackPanel>
            <Frame x:Name="MainFrame"/>
        </StackPanel>
    </Grid>
</Window>


